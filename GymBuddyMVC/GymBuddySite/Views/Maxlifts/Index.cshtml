@model IEnumerable<GymBuddySite.Models.Maxlift>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MaxBench)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaxSquat)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaxDeadLift)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Workout)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MaxBench)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaxSquat)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaxDeadLift)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Workout.Id)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.MaxliftsId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.MaxliftsId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.MaxliftsId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
