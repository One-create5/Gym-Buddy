@*Day 1*@
<tbody>
    @*Deadlifts*@
    @foreach (var item in Model.WorkoutExercises.Where(c => c.WorkoutExerciseId == 48))
    {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.Name)</td>
            <td>@Html.DisplayFor(modelItem => item.Sets)</td>
            <td>@Html.DisplayFor(modelItem => item.Reps)</td>
            <td>@Html.DisplayFor(modelItem => item.OneRepMax)%</td>
            <td>
                @{
                    var cal = (item.OneRepMax * (decimal).01) * Model.PowerliftWorkout.MaxDeadLift;
                }
                @String.Format("{0:0.00}", cal)
